    "version": "0.2.0",
    "configurations": [
        {
            "name": "CLI-run",
            "type": "python",
            "request": "launch",
            "module": "src.cli",
            "justMyCode": true,
            "args": ["run","-a","amd64:amd64","-i","gpt","-f","ufs","-e","none", "-v", "DEBUG"]
        },
        {
            "name": "CLI-setup",
            "type": "python",
            "request": "launch",
            "module": "src.cli",
            "justMyCode": true,
            "args": ["setup", "-v", "DEBUG"]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}